{
  "name": "voicegen-ai",
  "productName": "VoiceGen AI",
  "version": "1.0.0",
  "description": "Text-to-Speech app using Google Gemini AI",
  "main": "src/main.ts",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "build": "tsc && vite build",
    "dist": "npm run build && electron-builder"
  },
  "keywords": [
    "electron",
    "text-to-speech",
    "tts",
    "gemini",
    "ai",
    "voice"
  ],
  "author": "VoiceGen AI",
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.8.3",
    "@electron-forge/maker-deb": "^7.9.0",
    "@electron-forge/maker-dmg": "^7.9.0",
    "@electron-forge/maker-rpm": "^7.9.0",
    "@electron-forge/maker-squirrel": "^7.9.0",
    "@electron-forge/maker-zip": "^7.9.0",
    "@electron-forge/plugin-vite": "^7.9.0",
    "@electron-forge/shared-types": "^7.9.0",
    "@types/node": "^20.0.0",
    "@types/react": "19.1.13",
    "@types/react-dom": "19.1.9",
    "@types/wav": "^1.0.0",
    "electron": "^38.1.2",
    "electron-builder": "^26.0.12",
    "postcss": "8.5.6",
    "tailwindcss": "4.1.13",
    "typescript": "^5.0.0",
    "vite": "^7.1.6"
  },
  "dependencies": {
    "@electron/fuses": "^1.8.0",
    "@google/genai": "^1.20.0",
    "bufferutil": "^4.0.9",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "electron-squirrel-startup": "^1.0.1",
    "fs": "0.0.1-security",
    "lucide-react": "0.544.0",
    "next": "15.5.3",
    "path": "0.12.7",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "tailwind-merge": "3.3.1",
    "tailwindcss-animate": "1.0.7",
    "utf-8-validate": "^6.0.5",
    "util": "0.12.5",
    "wav": "^1.0.2"
  },
  "build": {
    "appId": "com.miempresa.voicegenai",
    "productName": "VoiceGen AI",
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "mac": {
      "target": "dmg",
      "icon": "build/icon.icns"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "linux": {
      "target": "deb",
      "icon": "build/icons"
    }
  }
}
